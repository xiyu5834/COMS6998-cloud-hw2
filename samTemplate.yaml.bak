AWSTemplateFormatVersion: '2010-09-09'
Transform: 'AWS::Serverless-2016-10-31'
Description: photo Lambda
Resources:
  hw2LF1:
    Type: 'AWS::Serverless::Function'
    Properties:
      FunctionName: hw2LF1new
      Handler: index-photos.lambda_handler
      Runtime: python3.9
      CodeUri: ./hw2LF1
      Description: 'Lambda function for photo index'
      MemorySize: 128
      Timeout: 30
      Role: 'arn:aws:iam::033200907874:role/service-role/hw2LF1-role-rfdpphgj'
      Environment:
        Variables:
          REGION: us-east-1
  hw2LF2:
    Type: 'AWS::Serverless::Function'
    Properties:
      FunctionName: hw2LF2new
      Handler: search-photos.lambda_handler
      Runtime: python3.7
      CodeUri: ./hw2LF2
      Description: 'Lambda function for photo search'
      MemorySize: 128
      Timeout: 30
      Role: 'arn:aws:iam::033200907874:role/service-role/hw2LF2-role-shon2zv0'
      Environment:
        Variables:
          REGION: us-east-1
  photoLexFulfill:
    Type: 'AWS::Serverless::Function'
    Properties:
      FunctionName: photoLexFulfillNew
      Handler: lambda_function.lambda_handler
      Runtime: python3.7
      CodeUri: ./photoLexFulfill
      Description: 'Lambda function helps for photo search'
      MemorySize: 128
      Timeout: 30
      Role: 'arn:aws:iam::033200907874:role/service-role/photoLexFulfill-role-yy9jhyxv'
      Environment:
        Variables:
          REGION: us-east-1
